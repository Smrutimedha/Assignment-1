#include <iostream>
using namespace std;
int main() {
	int i;
	double d;
	char c;
	cout << "Enter an integer" <<"\n";
	cin >> i;
	cout << "Enter a decimal" <<"\n";
	cin >> d;
	cout << "Enter a character" <<"\n";
	cin >> c;
	cout << i << " is an integer" <<"\n";
	cout << d << " is a decimal"<<"\n";
	cout << c << " is a character"<<"\n";
	return 0;
}
